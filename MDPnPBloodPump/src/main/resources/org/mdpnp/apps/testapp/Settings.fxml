<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="deviceTypeRow" fx:id="gridPane" alignment="CENTER"
	hgap="5.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="org.mdpnp.apps.testapp.SettingsController">

	<children>



		<Label fx:id="domainIdLabel" text="Domain Id"
			GridPane.columnIndex="0" GridPane.rowIndex="5">
			<GridPane.margin>
				<Insets bottom="5.0" top="5.0" />
			</GridPane.margin>
		</Label>
		<TextField fx:id="domainId" alignment="CENTER_RIGHT"
			maxWidth="40.0" minWidth="-1.0" prefColumnCount="2" prefWidth="30.0"
			promptText="" text="0" GridPane.columnIndex="1" GridPane.hgrow="NEVER"
			GridPane.rowIndex="5">

			<GridPane.margin>
				<Insets bottom="5.0" top="5.0" />
			</GridPane.margin>
		</TextField>
		<Label fx:id="fhirServerLabel" text="FHIR Server"
			GridPane.columnIndex="0" GridPane.rowIndex="6">
			<GridPane.margin>
				<Insets bottom="5.0" right="5.0" top="5.0" />
			</GridPane.margin>
		</Label>
		<TextField fx:id="fhirServer" maxWidth="200.0" prefWidth="200.0"
			promptText="FHIR URL for patient records" GridPane.columnIndex="1"
			GridPane.rowIndex="6" />
	</children>
	<columnConstraints>
		<ColumnConstraints halignment="RIGHT" hgrow="NEVER"
			minWidth="10.0" percentWidth="25.0" prefWidth="100.0" />
		<ColumnConstraints halignment="LEFT" hgrow="NEVER"
			minWidth="10.0" prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="0" prefHeight="-1"
			valignment="CENTER" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
		<RowConstraints minHeight="0" prefHeight="-1" vgrow="NEVER" />
	</rowConstraints>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</GridPane>
