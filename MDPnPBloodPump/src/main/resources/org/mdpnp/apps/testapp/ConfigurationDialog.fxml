<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="main" prefHeight="-1" prefWidth="-1"
	xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="org.mdpnp.apps.testapp.ConfigurationDialog">
	<bottom>
		<BorderPane prefHeight="-1.0" prefWidth="-1.0">
			<left>
				<Button fx:id="quit" cancelButton="true" mnemonicParsing="false"
					text="Quit" />
			</left>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" />
			</padding>
			<right>
				<Button fx:id="start" defaultButton="true" mnemonicParsing="false"
					text="Start" />
			</right>
			<BorderPane.margin>
				<Insets fx:id="insets" />
			</BorderPane.margin>
		</BorderPane>
	</bottom>
	<center>
		<fx:include fx:id="settings" source="Settings.fxml"
			BorderPane.alignment="TOP_CENTER" BorderPane.margin="$insets" />
	</center>
</BorderPane>
